---
title: "<%= post.display_title %>"
published: <%= post.published? %>
<% if post.published_at.present? -%>
published_at: <%= post.published_at.iso8601 %>
<% end -%>
<% if post.tag_list.present? -%>
tags: [<%= post.tag_list.map { |tag| "\"#{tag}\"" }.join(", ") %>]
<% end -%>
